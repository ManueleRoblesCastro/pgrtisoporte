extends ../default
block content

	include ../menu/navmenu
	include OTmodal

	div(class=['ui', 'grid', 'container'])
		div(class='row')

		div(class=['ui', menuactual.datamenuselect.ColorSemantic.toString().trim(), 'inverted',  'circular', 'segment', 'center', 'floated', 'eight', 'wide', 'column'])
				h4(class=['ui', 'header'])=menuactual.datamenuselect.AyudaMenu.toString().trim() + '-' + menuactual.datamenuselect.NombreMenu.toString().trim()
					i(class=['large', menuactual.datamenuselect.IconoSemantic.toString().trim(), 'icon'])
					
		//-div(class='ui vertical segment')
		div(class=['floated', 'center', 'aligned', 'two', 'wide', 'column','segment'] id='idestadosOT')		
			div(class=['ui text shape'] id='idccolors')
				div(class='sides')
					div(class=['active ui header side'])
						a(class=['ui black ribbon label'] id='idirestaso_10' onclick="iratableroestado('10', 'Todas')")='--Todos--'
					div(class=['ui header side'])
						a(class=['ui yellow ribbon label'] id='idirestaso_1' onclick="iratableroestado('1')")='En Curso'
					div(class=['ui header side'])
						a(class=['ui grey ribbon label'] id='idirestaso_3' onclick="iratableroestado('3')")='En Pausa'
					div(class=['ui header side'])
						a(class=['ui blue ribbon label'] id='idirestaso_4' onclick="iratableroestado('4')")='Repr. Fecha'
					div(class=['ui header side'])
						a(class=['ui green ribbon label'] id='idirestaso_5' onclick="iratableroestado('5')")='Finalizada'
					div(class=['ui header side'])
						a(class=['ui red ribbon label'] id='idirestaso_6' onclick="iratableroestado('6')")='Req. Autor.'
			a(class=['ui right button icon'] id='idirtableroleft')
				i(class=['arrow left icon'])
			a(class=['ui right button icon'] id='idirtableroright')
				i(class=['arrow right icon'])

		div(class=['floated', 'center', 'aligned', 'two', 'wide', 'column','segment'] id='idagregarOT')
			h4(class=['ui', 'icon', 'header'])
				i(class=['large', 'add to calendar', 'circular', 'icon'])
				div(class='content')='Agregar'						

		div(class=['floated', 'center', 'aligned', 'two', 'wide', 'column','segment'] id='regmenunivel' onclick="regresarmenunivel('"+menuactual.datamenuselect.CodigoMenuSuperior.toString().trim()+"')")
			h4(class=['ui', 'icon', 'header'])
				i(class=['large', 'step', 'backward', 'text', 'circular', 'icon'])
				div(class='content')='Regresar'

	input(type='hidden' name='codtecnico' id='idcodtecnico' value=user.CodigoTecnico.trim())
	input(type='hidden' name='conteoot' id='idconteoot')
	input(type='hidden' name='sumapesoot' id='idsumapesoot')
	
	div(class=['ui', 'grid', 'container','stackable'])
		div(class=['five wide column'])
	
			table(class=['ui','table','small'] id='tbl_ot_tab_est')
				thead
					tr
						th='Codigo'
						th='Nombre'
						th
							div(data-tooltip='Peso de Ordenes de Trabajo' data-position='center' data-inverted='')='Peso OT.'						
						th
							div(data-tooltip='Cantidad de Ordenes de Trabajo' data-position='center' data-inverted='')='Cantidad OT.'
						th='idEstado'
	
				tbody(id='myTableBody')
					each datotableroxtec in datostableroxtec
						tr
							td= datotableroxtec.CodigoTecnico
							td= datotableroxtec.NombreTecnico
							td= datotableroxtec.SumaPesoOT							
							td= datotableroxtec.ConteoOT
							td= datotableroxtec.idEstado
							
			div(class='row')
				div(class='content')
					div(class=['ui label'])='Total Peso: '
					a(class=['ui black label large circular'] id='idtotalpeso' data-tooltip='Total peso' data-position='bottom right' data-inverted='')
					div(class=['ui label'])='Total OT: '
					a(class=['ui black label large circular'] id='idtotalOT' data-tooltip='Total OT' data-position='bottom right' data-inverted='')					
	
		div(class=['one wide column'])
			div(class=['ui vertical divider'])
				i(class=['bar chart icon'])
	
		div(class=['ten wide column'])
	
			table(class=['ui small table'] id='tbl_det_ot_tab')
				thead
					tr
						th
							div(data-tooltip='Código de Orden de Trabajo' data-position='center' data-inverted='')='IdOT'
						th='Fecha'
						th='Resumen'
						th='Peso'
						th='Unidad'
						th='Fecha.Sol'					
						th='Activo Fijo'
						th='Prioridad'
						th='IdEstado'
						th='CodigoTecnico'
				tbody(id='myTableBody1')

		script(type='text/javascript').

			function regresarmenunivel(codigomenu) {
				window.location.href ='/menu/nav/'+codigomenu;
			};
			
			function iratableroestado(codigoestadoot) {
				//alert('Estado:' + codigoestadoot);			
				$.get( '/pantallas/tableroxestado/'+codigoestadoot, function( objJsonDatosRec ) {
					$('#tbl_ot_tab_est').DataTable().rows().remove().draw();
					$('#tbl_det_ot_tab').DataTable().rows().remove().draw();
					console.log(objJsonDatosRec.datostableroxtec);
					if (objJsonDatosRec.datostableroxtec.length>0){
						$('#tbl_ot_tab_est').DataTable().rows.add(objJsonDatosRec.datostableroxtec).draw();
					}
				});				
			};
			
			$('#idirtableroleft').on('click',function(){
				$('#idccolors').shape('flip right');
			});
			$('#idirtableroright').on('click',function(){
				$('#idccolors').shape('flip left');
			});	

			$('#tbl_ot_tab_est').DataTable({
				"dom": '<"top"p>rt<"bottom"><"clear">',

				columns: [
					{ data: 'CodigoTecnico', width: 5, visible: false },
					{ data: 'NombreTecnico', width: 10 },
					{ data: 'SumaPesoOT', width: 5},					
					{ data: 'ConteoOT', width: 5 },
					{ data: 'idEstado', width: 10,  visible: false}
				],

				colReorder: true,
				responsive: true,
				paging: true,
				select: {
					style: 'single'
				},
				'pageLength': 10,
				 "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, "todo"]],
				'order': [[ 0, 'desc' ]],
				stateSave: true,
				'language': {
					url: 'https://cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Spanish.json',
					select: {
						rows: "%d fila(s) seleccionada(s)"
					}
				},
				"autoWidth": true,
				"rowCallback": function( row, data, index ) {
					if(index%2 == 0){
						$(row).removeClass('myodd myeven');
						$(row).addClass('myodd');
						}
					else{
						$(row).removeClass('myodd myeven');
						$(row).addClass('myeven');
					}
				},

				drawCallback: function () {
					var api = this.api();
					var value2int =parseInt(api.column( 2 ).data().sum());
					$('a#idtotalpeso').text(value2int.toString());
					var value2int1 =parseInt(api.column( 3 ).data().sum());
					$('a#idtotalOT').text(value2int1.toString());					
				}

			});
			
			$('#tbl_ot_tab_est tbody').on('click', 'td', function () {

				var cellclick = this;
				var FSelected = [];
				var row_clicked = $(this).closest('tr');
				var num_fila = $('#tbl_ot_tab_est').DataTable().row(row_clicked).index();
				//alert('Fila ' + num_fila);
				var rowData = $('#tbl_ot_tab_est').DataTable().row(row_clicked).data();
				FSelected = JSON.stringify(rowData);
				FSelected = JSON.parse(FSelected);

				var col_clicked = $('#tbl_ot_tab_est').DataTable().cell( this ).index().columnVisible;
				//alert('Columna ' + col_clicked);

				if (col_clicked ==0){
					if (FSelected.CodigoTecnico!=undefined && FSelected.CodigoTecnico!=''){
						//alert('tec est ' + FSelected.CodigoTecnico + ' ' + FSelected.idEstado);
						$.get( '/pantallas/tblxestadoxtec/'+FSelected.idEstado+'/'+FSelected.CodigoTecnico, function( objJsonDatosRec ) {
							$('#tbl_det_ot_tab').DataTable().rows().remove().draw();
							$('#tbl_det_ot_tab').DataTable().rows.add(objJsonDatosRec.datootfiltered).draw();
						});
					}
				}
			});

			$('#tbl_det_ot_tab').DataTable({
				"dom": '<"top"fil>rt<"clear"><"bottom"pB>',

				columns: [
					{ data: 'IdOT', width:10 },
					{ data: 'FechaOT', width: 10 },
					{ data: 'Resumen', width: 50 },
					{ data: 'Peso', width: 10, "sClass": "right aligned" },
					{ data: 'NombreUnidad', width: 20 },
					{ data: 'FechaResolucion', width: 10 },
					{ data: 'NombreActivoFijo', width: 20 },
					{ data: 'NombrePrioridad', width: 10 },
					{ data: 'IdEstado', width: 10 , visible: false},
					{ data: 'CodigoTecnico', width: 10, visible: false }
				],

				colReorder: true,
				responsive: true,			
				paging: true,
				select: {
					style: 'single'
				},
				'pageLength': 5,
				 "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, "todo"]],
				'order': [[ 0, 'desc' ]],
				stateSave: true,
				'language': {
					url: 'https://cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Spanish.json',
					select: {
						rows: "%d fila(s) seleccionada(s)"
					}
				},

				"rowCallback": function( row, data, index ) {

					if(data.IdEstado==1){
						$(row).removeClass('amarillocrs');
						$(row).addClass('amarillocrs');
					}
					if(data.IdEstado==3){
						$(row).removeClass('griscrs');
						$(row).addClass('griscrs');
					}
					if(data.IdEstado==4){
						$(row).removeClass('celestecrs');
						$(row).addClass('celestecrs');
					}
					if(data.IdEstado=='5'){
						$(row).removeClass('verdecrs');
						$(row).addClass('verdecrs');
					}					
					if(data.IdEstado=='6'){
						$(row).removeClass('rojocrs');
						$(row).addClass('rojocrs');
					}
				},
				"autoWidth": true,
				buttons: [
					{
					title: 'Ordenes de Trabajo',
					extend: 'pdfHtml5',
					filename: 'OT_'+ $('#idcodtecnico').val(),
					text: 'PDF',

					customize: function (doc) {
						var logo = "data:image/jpeg;base64,";

						doc.pageMargins = [20,60,20,30];
						doc.defaultStyle.fontSize = 8;
						doc.styles.tableHeader.fontSize = 8;

						var vtoday = new Date();
						var vdd = vtoday.getDate();
						var vmm = vtoday.getMonth()+1;
						var vyyyy = vtoday.getFullYear();
						if(vdd<10) { vdd = '0'+vdd } 
						if(vmm<10) { vmm = '0'+vmm } 
						vtoday =  vdd + '-' + vmm + '-' + vyyyy;

						doc['header']=(function() {
							return {
								columns: [
									{
										height: 37,
										image: logo,
										width: 37
									},
									{
										alignment: 'left',
										italics: true,
										//text: 'dataTables',
										text: 'Soporte Tecnico PGR',
										fontSize: 12,
										margin: [10,0]
									},
									{
										alignment: 'right',
										fontSize: 10,
										//text: 'Custom PDF export with dataTables'
										text: 'Fecha de Impresión: ' + vtoday
									}
								],
								margin: 22
								}
							});
						}
					},

					{
						title:  $('#idcodtecnico').val() + ', Ordenes de Trabajo ',
						extend: 'excelHtml5',
						filename: 'Corte_'+ $('#idcodtecnico').val(),
						text: 'Excel',
						customize: function(xlsx) {
						var sheet = xlsx.xl.worksheets['sheet1.xml'];
						// Loop over the cells in column `F`
						$('row c[r^="F"]', sheet).each( function () {
						// Get the value and strip the non numeric characters
						if ( $('is t', this).text().replace(/[^\d]/g, '') * 1 >= 500000 ) {
								$(this).attr( 's', '20' );
								}
							});
						}
					}
				]
			});

			$('#idagregarOT').on('click',function(){
			
				$('#id_operacion').val(1);
				//alert($('#id_operacion').val());
						
				$('#modalOT').modal({
					closable : false,
					onShow: function(){					
						$('#id_FechaOT').flatpickr(
							{
								dateFormat: "Y/m/d",
								//enableTime: true,
								locale: 'es'
							}
						);
						$('#id_FechaResolEsp').flatpickr(
							{
								dateFormat: "Y/m/d",
								//enableTime: true,
								locale: 'es'
							}
						);
						
						$.get( '/pantallas/addOTs/1/0', function( objJsonDatosRec ) {

							$('#id_FechaOT').val(objJsonDatosRec.fechaDefault);
							$('#id_FechaOT').prop('readonly', false);
							$('#id_FechaResolEsp').val(objJsonDatosRec.fechaDefault);
							$('#id_FechaResolEsp').prop('readonly', false);

							var listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.empleadoslist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.empleadoslist[i].IdMarca + "'>" + objJsonDatosRec.empleadoslist[i].NombreEmpleado + "</option>";
							}
							$("#id_IdMarca").html(listItems);
							$('#id_IdMarca').dropdown('set selected', '0');							
							
							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.unidadeslist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.unidadeslist[i].IdUnidad + "'>" + objJsonDatosRec.unidadeslist[i].NombreUnidad + "</option>";
							}
							$("#id_IdUnidad").html(listItems);
							$('#id_IdUnidad').dropdown('set selected', '48');
													
							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.lugareslist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.lugareslist[i].IdLugar + "'>" + objJsonDatosRec.lugareslist[i].NombreLugar + "</option>";
							}
							$("#id_IdLugar").html(listItems);
							$('#id_IdLugar').dropdown('set selected', '3');

							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.activofijoslist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.activofijoslist[i].CodigoActivoFijo + "'>" + objJsonDatosRec.activofijoslist[i].NombreActivoFijo + "</option>";								
							}
							$("#id_CodigoActivoFijo").html(listItems);
							$('#id_CodigoActivoFijo').dropdown('set selected', '0000-00000-000-0-0000');

							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.prioridadeslist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.prioridadeslist[i].IdPrioridad + "'>" + objJsonDatosRec.prioridadeslist[i].NombrePrioridad + "</option>";
							}
							$("#id_IdPrioridad").html(listItems);
							$('#id_IdPrioridad').dropdown('set selected', '3');

							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.estadoslist.length; i++){
								if(objJsonDatosRec.estadoslist[i].IdEstado=='1'){
									listItems+= "<option value='" + objJsonDatosRec.estadoslist[i].IdEstado + "'>" + objJsonDatosRec.estadoslist[i].NombreEstado + "</option>";
								}
							}							
							$("#id_IdEstado").html(listItems);
							$('#id_IdEstado').dropdown('set selected', '1');							

							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.tiposlist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.tiposlist[i].IdTipo_OT + "'>" + objJsonDatosRec.tiposlist[i].NombreTipoOT + "</option>";
							}
							$("#id_IdTipo_OT").html(listItems);
							$('#id_IdTipo_OT').dropdown('set selected', '0');
							
							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.subtiposlist.length; i++){									
								if(objJsonDatosRec.subtiposlist[i].IdTipo_OT==$("#id_IdTipo_OT").val()){
									listItems+= "<option value='" + objJsonDatosRec.subtiposlist[i].IdSubTipo_OT + "'>" + objJsonDatosRec.subtiposlist[i].Descripcion + "</option>";
								}else{
									listItems+= "<option value='" + objJsonDatosRec.subtiposlist[i].IdSubTipo_OT + "' disabled>" + objJsonDatosRec.subtiposlist[i].Descripcion + "</option>";
								}
							}
							$('#id_IdSubTipo_OT').html(listItems);
							$('#id_IdSubTipo_OT').dropdown('set selected', '0');

							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.categoriaslist.length; i++){
								if(objJsonDatosRec.categoriaslist[i].IdSubTipo_OT==$('#id_IdSubTipo_OT').val()){
									listItems+= "<option value='" + objJsonDatosRec.categoriaslist[i].IdCategoria_OT + "'>" + objJsonDatosRec.categoriaslist[i].Descripcion + "</option>";
								}else{
									listItems+= "<option value='" + objJsonDatosRec.categoriaslist[i].IdCategoria_OT + "' disabled>" + objJsonDatosRec.categoriaslist[i].Descripcion + "</option>";
								}
							}
							$('#id_IdCategoria_OT').html(listItems);
							$('#id_IdCategoria_OT').dropdown('set selected', '0');

							$("#id_IdTipo_OT").change(function(){
								listItems= "", i=0;
								for (i = 0; i < objJsonDatosRec.subtiposlist.length; i++){									
									if(objJsonDatosRec.subtiposlist[i].IdTipo_OT==$("#id_IdTipo_OT").val() || parseInt(objJsonDatosRec.subtiposlist[i].IdSubTipo_OT)==0 ){
										listItems+= "<option value='" + objJsonDatosRec.subtiposlist[i].IdSubTipo_OT + "'>" + objJsonDatosRec.subtiposlist[i].Descripcion + "</option>";
									}else{
										listItems+= "<option value='" + objJsonDatosRec.subtiposlist[i].IdSubTipo_OT + "' disabled>" + objJsonDatosRec.subtiposlist[i].Descripcion + "</option>";
									}
								}
								$('#id_IdSubTipo_OT').html(listItems);
								$('#id_IdSubTipo_OT').dropdown('set selected', '0');
								$('#id_Peso').val(0);
							});
							
							$("#id_IdSubTipo_OT").change(function(){
								listItems= "", i=0;
								for (i = 0; i < objJsonDatosRec.categoriaslist.length; i++){									
									if(objJsonDatosRec.categoriaslist[i].IdSubTipo_OT==$("#id_IdSubTipo_OT").val() || parseInt(objJsonDatosRec.categoriaslist[i].IdCategoria_OT)==0 ){
										listItems+= "<option value='" + objJsonDatosRec.categoriaslist[i].IdCategoria_OT + "'>" + objJsonDatosRec.categoriaslist[i].Descripcion + "</option>";
									}else{
										listItems+= "<option value='" + objJsonDatosRec.categoriaslist[i].IdCategoria_OT + "' disabled>" + objJsonDatosRec.categoriaslist[i].Descripcion + "</option>";
									}
								}
								$('#id_IdCategoria_OT').html(listItems);
								$('#id_IdCategoria_OT').dropdown('set selected', '0');
								for(i = 0; i < objJsonDatosRec.subtiposlist.length; i++){
									if(objJsonDatosRec.subtiposlist[i].IdSubTipo_OT==$(this).val()){
										$('#id_Peso').val(objJsonDatosRec.subtiposlist[i].Peso);
									}
								}
							});

							$("#id_IdCategoria_OT").change(function(){
								for(i = 0; i < objJsonDatosRec.categoriaslist.length; i++){
									if(objJsonDatosRec.categoriaslist[i].IdCategoria_OT==$(this).val()){
										$('#id_Peso').val(objJsonDatosRec.categoriaslist[i].Peso);
									}
								}								
							});														
																					
							listItems= "", i=0;
							for (i = 0; i < objJsonDatosRec.tecnicoslist.length; i++){
								listItems+= "<option value='" + objJsonDatosRec.tecnicoslist[i].CodigoTecnico + "'>" + objJsonDatosRec.tecnicoslist[i].NombreTecnico + "</option>";
							}
							$("#id_CodigoTecnico").html(listItems);
							$('#id_CodigoTecnico').dropdown('set selected', 'HARIVASG');
							
							$('#id_Resumen').val('');
							$('#id_Peso').val('0');
							$('#id_IdOT').val('0');
						});
						
					},
					onHide: function(){
					}					
				});
				$('#modalOT').modal('show');
				return;

			});
			
			$('#tbl_det_ot_tab tbody').on('click', 'td', function () {

				var cellclick = this;
				var FSelected = [];
				var row_clicked = $(this).closest('tr');
				var num_fila = $('#tbl_det_ot_tab').DataTable().row(row_clicked).index();
				var rowData = $('#tbl_det_ot_tab').DataTable().row(row_clicked).data();
				FSelected = JSON.stringify(rowData);
				FSelected = JSON.parse(FSelected);

				var col_clicked = $('#tbl_det_ot_tab').DataTable().cell( this ).index().columnVisible;

				if (col_clicked ==0){
					if (FSelected.IdOT!=undefined && FSelected.IdOT>0){
						$('#id_operacion').val(2);				
						//alert('IdOT  ' + FSelected.IdOT + ' Operacion ' + $('#id_operacion').val());
						
						$('#modalOT').modal({
							closable : false,
							onShow: function(){
								$('#id_FechaOT').flatpickr(
									{
										dateFormat: "Y/m/d",
										locale: 'es'
									}
								);
								$('#id_FechaResolEsp').flatpickr(
									{
										dateFormat: "Y/m/d",
										locale: 'es'
									}
								);

								$.get( '/pantallas/addOTs/2/'+FSelected.IdOT, function( objJsonDatosRec ) {

									$('#id_IdOT').val(objJsonDatosRec.OTupdate[0].IdOT);
									$('#id_FechaOT').val(objJsonDatosRec.OTupdate[0].FechaOT);
									$('#id_FechaResolEsp').val(objJsonDatosRec.OTupdate[0].FechaResolEsp);

									var listItems= "", i=0;
									for (i = 0; i < objJsonDatosRec.empleadoslist.length; i++){
